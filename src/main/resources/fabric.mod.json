{
  "schemaVersion": 1,
  "id": "originenforcer",
  "version": "${version}",

  "name": "Origin Enforcer",
  "description": "A mod that assigns Origins to players from a config file, ignoring which Origin they chose.",
  "authors": [
    "SansSerif"
  ],
  "contact": {
    "homepage": "https://www.youtube.com/watch?v=dQw4w9WgXcQ",
    "sources": "https://github.com/SansSerif1/OriginEnforcer"
  },

  "license": "GPLv3.0",
  "icon": "assets/originenforcer/icon.png",

  "environment": "*",
  "entrypoints": {
    "server": [
      "me.sansserif.originenforcer.OriginEnforcerMod"
    ],
    "client": [
      "me.sansserif.originenforcer.ClientNotice"
    ]
  },
  "mixins": [
    "originenforcer.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "origins": ">=1.1.0"
  }
}
